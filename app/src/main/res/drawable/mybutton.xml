<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">



        <item android:state_pressed="true" >
            <shape android:shape="rectangle"  >
                <corners android:radius="3dip" />
                <stroke android:width="1dip" android:color="#f2f7f6" />
                <gradient android:angle="-90" android:startColor="#f2f7f6" android:endColor="#689a92"  />
            </shape>
        </item>
        <item android:state_focused="true">
            <shape android:shape="rectangle"  >
                <corners android:radius="3dip" />
                <stroke android:width="1dip" android:color="#f2f7f6" />
                <solid android:color="#f2f7f6"/>
            </shape>
        </item>
        <item >
            <shape android:shape="rectangle"  >
                <corners android:radius="3dip" />
                <stroke android:width="1dip" android:color="#f2f7f6" />
                <gradient android:angle="-90" android:startColor="#8dbab3" android:endColor="#f2f7f6" />
            </shape>
        </item>
    </selector>


